{% for image in imageList %}
    <div class="screenshot-list--image" style="background-image:url('https://pvd.onl/pages/getimage.php?file={{ image }}');">
        <div class="screenshot-list--image-container">
            <div class="screenshot-list--image-buttons">
                <i class="fa fa-external-link-alt fa-2x dropOpen" data-image="{{ image }}"></i>
                <i class="fa fa-clipboard fa-2x dropCopy" data-clipboard-text="https://pvd.onl/{{ image }}"></i>
                <i class="fa fa-eye-slash fa-2x dropHide" data-image="{{ image }}"></i>
                <i class="fa fa-trash fa-2x dropRemove" data-image="{{ image }}"></i>
            </div>
        </div>
    </div>
{% endfor %}
