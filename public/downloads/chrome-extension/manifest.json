{
  "name": "Feednews",
  "version": "1",
  "browser_action":
  {
    "default_icon": "feednews.png"
  },
  "background":
  {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "notifications",
    "https://feednews.me/chrome/import/",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "<all_urls>"
  ],
  "icons": {
    "16": "feednews.png"
  },
  "web_accessible_resources": [
    "feednews.png"
  ],
  "manifest_version": 2
}
